generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Ping {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
}

model Order {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  userId    String
  username  String?
  item      String
  qty       Int
}
